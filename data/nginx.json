	"lua_red_6": {"events": {"worker_connections": 4096} },
	"lua_red_7": {
		"http": {
			"lua_red_1": {
				"include": ["include conf/mime.types",			
					"include /etc/nginx/proxy.conf",
					"include /tmp/myngixservers/*.conf",
					"include  /etc/nginx/fastcgi.conf"
					]
			},"lua_red_2": {
				"index": "index.html index.html index.php"
			},"lua_red_3": {
				"default_type": "application/octet-stream"
			},"lua_red_4": {
				"log_format": [
					"main '$remote_addr - $remote_user [$time_local]  $status '","'\"$request\" $body_bytes_sent \"$http_referer\" '",
					"'\"$http_user_agent\" \"$http_x_forwarded_for\"'"
